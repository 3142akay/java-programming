public class CHW_04_literals
{
    public static void main(String[] args) {
        byte age = 34;
        int age2 = 56;
        short age3 = 45;
        long ageDino = 108387777l;
        char ch = 'A';
        float f1 = 44.67f;
        double d1 = 4.66;
        boolean a = true ;
        System.out.print(age);
        String str = "Harry";
        System.out.println(str);

    }
}
