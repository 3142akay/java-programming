public class Percentage_Calculation {
    public static void main(String[] args) {
        float subject1 = 86;
        float subject2 = 84;
        float subject3 = 82;
        float subject4 = 75;
        float subject5 = 78;
        float percentage = (subject1+subject2+subject3+subject4+subject5)/500*100;
        System.out.println(percentage);

    }
}





